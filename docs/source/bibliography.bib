 publisher = {American Society of Mechanical Engineers Digital Collection},
 title = {Performance {Prediction} of {Axial} {Flow} {Two}-{Stage} {Turbine} {Based} on {Different} {Losses} {Models}},
 urldate = {2024-02-26},
 year = {2013}
